## The function is currently defined as
function (vec, from, to) 
{
    library(plyr)
    if (length(from) != length(to)) 
        stop("to and from must be of the same length")
    ix <- which(from \%in\% vec)
    from <- from[ix]
    to <- to[ix]
    ix <- laply(from, function(x) which(vec == x))
    vec[ix] <- to
    vec
  }
}
